{
  "expo": {
    "name": "meetnow",
    "slug": "meetnow",
    "scheme": "meetnow",
    "owner": "gregoriusjimmy",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gregoriusjimmy.meetnow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.gregoriusjimmy.meetnow"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [["expo-image-picker"]]
  }
}
