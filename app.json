{
  "expo": {
    "name": "meetnow",
    "slug": "meetnow",
    "scheme": "meetnow",
    "owner": "meetnow",
    "currentFullName": "meetnow/meetnow",
    "originalFullName": "meetnow/meetnow",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "build": {
      "preview": {
        "android": {
          "buildType": "apk"
        }
      },
      "preview2": {
        "android": {
          "gradleCommand": ":app:assembleRelease"
        }
      },
      "preview3": {
        "developmentClient": true
      },
      "production": {}
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/8694a218-93fe-4cb6-81ba-1c98ad27fafb"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gregoriusjimmy.meetnow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.gregoriusjimmy.meetnow"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [["expo-image-picker"]],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "8694a218-93fe-4cb6-81ba-1c98ad27fafb"
      }
    }
  }
}
